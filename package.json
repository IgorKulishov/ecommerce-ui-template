{
  "name": "E-Commerce",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "build-prod": "ng build --prod=true --stats-json",
    "build-local-dev": "ng build --configuration=dev",
    "build-local-prod": "ng build --configuration=production && npm run minify",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d docs/",
    "e2e": "ng e2e",
    "heroku-postbuild": "ng build --prod=true",
    "lint": "ng lint",
    "minify": "node minify.build.js",
    "ng": "./node_modules/@angular/cli/bin/ng",
    "start": "node server.js",
    "start-mock": "ng serve --environment=mock",
    "stats": "ng build --prod --stats-json &&  npx webpack-bundle-analyzer dist/stats.json",
    "static-serve": "npm run build-local-dev && cd dist && live-server --port=4200 --host=localhost --entry-file=/index.html",
    "test": "ng test"
  },
  "private": true,
